{
  "name": "client",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "start:dev": "concurrently '(cd ./client && yarn start:dev)' '(cd ./server && yarn start:dev)'",
    "build": "concurrently '(cd ./client && yarn build)' '(cd ./server && yarn build)'",
